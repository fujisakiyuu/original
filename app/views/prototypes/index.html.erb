<main class="main">
<div class="main-contents">
 <div class="contents">
 <h1>
  検索結果
</h1>
<%= search_form_for @p, url: prototypes_path do |f| %>
  <%= f.label :catch_copy_eq, 'ジャンル' %>
  <%= f.collection_select :catch_copy_eq, @product_name, :catch_copy, :catch_copy,  include_blank: '指定なし' %>
  <br>
  <%= f.submit '検索' %>
<% end %>
 
 </div>
</div>
  <div class="inner">
     <% if user_signed_in? %>
       <div class="greeting">  
         <%= link_to "#{current_user.name}さん", user_path(current_user), class: :greeting__link %>
       </div> 
        <% end %>
    
    <div class="card__wrapper">
      <%= render partial: 'prototype', collection: @results %>
    </div>
  </div>
</main>
